{
    "name" : "Zemi",
    "version": "1.0.0",
    "description": "An extension to download images in bulk from message boards or websites",
    "manifest_version": 3,
    "author": "Bobby Boushe",
	"permissions" : [ 
        "tabs",
        "activeTab",
        "scripting"
     ],
     "background": {
        "service_worker": "popup.js"
     },
    "icons": {
        "16": "img/icons/icon16.png",
        "48": "img/icons/icon48.png",
        "128": "img/icons/icon128.png"
     },
    "action": {
        "default_title": "Zemi",
        "default_icon": "img/icons/icon.png",
        "default_popup": "index.html"
    }
}